{
  "name": "nest-nguniversal",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "build:all": "nx run-many --all --target=build",
    "lint:all": "nx run-many --all --target=lint --fix",
    "test:all": "nx run-many --all --target=test",
    "prepare": "husky install",
    "format": "nx format:write --all",
    "lint:affected": "nx affected:lint --base origin/develop",
    "test:affected": "nx affected:test --base origin/develop",
    "build:affected": "nx affected:build --base origin/develop",
    "c": "git-cz"
  },
  "private": true,
  "dependencies": {
    "@nestjs/common": "^9.0.7",
    "@nestjs/core": "^9.0.7",
    "@nguniversal/express-engine": "^14.0.3",
    "domino": "^2.1.6",
    "memory-cache": "^0.2.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@jscutlery/semver": "^2.25.2",
    "@nrwl/cli": "14.5.1",
    "@nrwl/eslint-plugin-nx": "14.5.1",
    "@nrwl/jest": "14.5.1",
    "@nrwl/js": "14.5.1",
    "@nrwl/linter": "14.5.1",
    "@nrwl/nx-cloud": "14.2.0",
    "@nrwl/web": "^14.5.1",
    "@nrwl/workspace": "14.5.1",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.1",
    "@types/memory-cache": "^0.2.2",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "^2.26.0",
    "git-cz": "^4.9.0",
    "husky": "^8.0.1",
    "jest": "27.5.1",
    "lint-staged": "^13.0.3",
    "ngx-deploy-npm": "^4.1.2",
    "nx": "14.5.1",
    "prettier": "2.7.1",
    "ts-jest": "27.1.4",
    "ts-node": "~10.8.0",
    "typescript": "4.7.4"
  },
  "lint-staged": {
    "*.{ts,json,html,tsx,js}": "yarn format"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  }
}
