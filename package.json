{
  "name": "nest-nguniversal",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "build:all": "nx run-many --all --target=build",
    "lint:all": "nx run-many --all --target=lint --fix",
    "test:all": "nx run-many --all --target=test",
    "prepare": "husky install",
    "format": "nx format:write --all",
    "lint:affected": "nx affected:lint --base origin/develop",
    "test:affected": "nx affected:test --base origin/develop",
    "build:affected": "nx affected:build --base origin/develop",
    "c": "git-cz"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^15.0.4",
    "@nestjs/common": "^9.2.1",
    "@nestjs/core": "^9.2.1",
    "@nguniversal/express-engine": "^15.0.0",
    "domino": "^2.1.6",
    "memory-cache": "^0.2.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@jscutlery/semver": "^2.25.2",
    "@nrwl/cli": "15.4.2",
    "@nrwl/eslint-plugin-nx": "15.4.2",
    "@nrwl/jest": "15.4.2",
    "@nrwl/js": "15.4.2",
    "@nrwl/linter": "15.4.2",
    "@nrwl/nx-cloud": "15.0.2",
    "@nrwl/web": "15.4.2",
    "@nrwl/workspace": "15.4.2",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/memory-cache": "^0.2.2",
    "@types/node": "18.7.1",
    "@typescript-eslint/eslint-plugin": "5.47.1",
    "@typescript-eslint/parser": "5.47.1",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "^2.26.0",
    "git-cz": "^4.9.0",
    "husky": "^8.0.1",
    "jest": "28.1.3",
    "jest-environment-jsdom": "^29.3.1",
    "lint-staged": "^13.0.3",
    "ngx-deploy-npm": "^4.1.2",
    "nx": "15.4.2",
    "prettier": "2.7.1",
    "ts-jest": "28.0.8",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "lint-staged": {
    "*.{ts,json,html,tsx,js,md}": "yarn format"
  }
}
